                                },
                                "status": {
                                    "type": "integer",
                                    "example": 400
                                }
                            },
                            "type": "object"
                        }
                    }
                }
            },
            "ForBidden": {
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
